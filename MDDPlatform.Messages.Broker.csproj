<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>MDDPlatform.Messages.Broker</PackageId>
    <Version>2.0.0</Version>
    <Authors>Adel Vahdati</Authors>
    <Description>Message Bus Library</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTag>Message Bus, Publisher, Subscriber, RabbitMQ</PackageTag>
    <RepositoryUrl>https://github.com/adelvahdati/MDDPlatform.Messages.Broker</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MDDPlatform.Messages.Dispatcher" Version="2.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="6.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="RabbitMQ.Client" Version="6.2.2" />
  </ItemGroup>

</Project>
